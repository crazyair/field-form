{
  "version": 2,
  "name": "rc-field-form",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": ".doc" }
    }
  ],
  "routes": [
    {
      "src": "/(.*[.js|.css])",
      "headers": { "Cache-Control": "max-age=63072000" },
      "continue": true
    },
    { "src": "/(.*)", "dest": "/dist/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/dist/index.html" }
  ],
  "github": {
    "enable": false
  }
}
